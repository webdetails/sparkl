        <class_source>import org.pentaho.platform.api.engine.IPentahoSession;
import org.pentaho.platform.api.engine.IPluginManager;
import org.pentaho.platform.engine.core.system.PentahoSessionHolder;
import org.pentaho.platform.engine.core.system.PentahoSystem;
import java.net.URLClassLoader;
import java.io.IOException;
import java.io.Closeable;
import java.lang.reflect.Field;
import java.lang.reflect.Method;
import java.util.ArrayList;
import java.util.HashMap;
import java.util.Map;
import java.util.HashSet;
import java.util.Vector;
import java.util.Iterator;
import java.util.Collections;
import java.util.jar.JarFile;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException{
  String pluginId = getParameter("pluginId");
  unloadPlugin(pluginId);
  setOutputDone();
  return false;
}

private void unloadPlugin(String pluginId){
  IPentahoSession session = PentahoSessionHolder.getSession();
  IPluginManager pluginManager = (IPluginManager)PentahoSystem.get( IPluginManager.class, session );
  ClassLoader cl = pluginManager.getClassLoader( pluginId );
  if ( cl != null &amp;&amp; cl instanceof URLClassLoader ) {
    try {
      URLClassLoader cl1 = (URLClassLoader) cl;
      clearURLClassLoader( cl1 );
      Method closeMethod = cl1.getClass().getMethod( "close", null );
      closeMethod.invoke( cl1, null );
    } catch ( Throwable e ) {
      if ( e instanceof IOException ) {
        logBasic( "Unable to close class loader for plugin. Will try uninstalling plugin anyway");
      } else {
        logBasic( "Error while closing class loader");
      }
    }
  }
}

/**
  * This method is designed to clear out classloader file locks in windows.
  *
  * @param clazzLdr class loader to clean up
  */
private void clearURLClassLoader( URLClassLoader clazzLdr ) {
  try {
    Object obj = getFieldObject( URLClassLoader.class, "ucp", clazzLdr );
    ArrayList loaders = (ArrayList) getFieldObject( obj.getClass(), "loaders", obj );
  for( Iterator it = loaders.iterator(); it.hasNext(); ){
        Object ldr = it.next();
        JarFile file = (JarFile) getFieldObject( ldr.getClass(), "jar", ldr );
        quietClose( file );
      }
  } catch ( Exception e ) {
    // skip
  }

  try {
    Vector nativeLibs = (Vector) getFieldObject( ClassLoader.class, "nativeLibraries", clazzLdr );
    for ( Iterator it = nativeLibs.iterator(); it.hasNext(); ) {
      Object nativeLib = it.next();
      try {
        Method finalizeMethod = nativeLib.getClass().getDeclaredMethod( "finalize", new Class[ 0 ] );
        finalizeMethod.setAccessible( true );
        finalizeMethod.invoke( nativeLib, new Object[ 0 ] );
      } catch ( Exception e ) {
        // skip
      }
    }
  } catch ( Exception e ) {
    // skip
  }

  HashMap urlCache = null;
  HashMap fileCache = null;

  try {

    Class factory =
        getFieldObject( classForName( "sun.net.www.protocol.jar.JarURLConnection" ), "factory", null )
        .getClass();
    urlCache = getMap( factory, "urlCache" );
    fileCache = getMap( factory, "fileCache" );

    for ( Iterator it = ( (HashMap) urlCache.clone() ).keySet().iterator(); it.hasNext(); ) {
      Object file = it.next();
      if ( file instanceof JarFile ) {
        JarFile jar = (JarFile) file;
        quietClose( jar );
        fileCache.remove( urlCache.get( jar ) );
        urlCache.remove( jar );
      }
    }
    for ( Iterator it = ( (HashMap) fileCache.clone() ).keySet().iterator(); it.hasNext(); ) {
      Object key = it.next();
      Object file = fileCache.get( key );
      if ( file instanceof JarFile ) {
        quietClose( (JarFile) file );
        fileCache.remove( key );
      }
    }
  } catch ( Exception e ) {
    // skip
  }
}

private void quietClose(Closeable closeable ){
  try {
    closeable.close();
  } catch ( IOException e ) {
    // skip
  }
}

private  HashMap  getMap( Class clazz, String fieldName ) {
  HashMap objMap = null;
  try { 
      objMap = (HashMap) getFieldObject( clazz, fieldName, null );
    } catch ( Exception e ) {
      // skip
    }

    if( objMap == null ) {
      return new HashMap();
    }

    return objMap;
}

private Object getFieldObject( Class clazz, String name, Object obj ) throws Exception {
  Field field = clazz.getDeclaredField( name );
  field.setAccessible( true );
  return field.get( obj );
}

private Class classForName( String name ) throws ClassNotFoundException {
  try {
    ClassLoader contextClassLoader = Thread.currentThread().getContextClassLoader();
    if ( contextClassLoader != null ) {
      return contextClassLoader.loadClass( name );
    }
  } catch ( Throwable ignore ) { }
  return Class.forName( name );
}</class_source>
